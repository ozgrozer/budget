    "concurrently": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.16",
    "nodemon": "^2.0.4",
    "parcel-bundler": "1.11.0",
    "pug": "^3.0.0",
